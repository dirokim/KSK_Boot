<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.winter.app.board.notice.NoticeDAO">
  	<select id="getList"  resultType="NoticeVO" parameterType="NoticeVO">
  		SELECT * FROM NOTICE 
  		WHERE NOTICECONTENTS LIKE CONCAT('%','3','%')
  		ORDER BY NOTICENUM DESC
  		LIMIT 0,10
  	</select>
  
  	<insert id="add" parameterType="NoticeVO">
  		INSERT INTO NOTICE (NOTICEWRITER,NOTICEHEAD,NOTICECONTENTS,NOTICEDATE,NOTICEVIEWS)
  		 VALUES(#{noticeWriter},#{noticeHead},#{noticeContents},now(),0)
  	</insert>
  
	  
  
  </mapper>
